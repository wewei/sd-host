# Environment variables for SD-Host
# Copy this file to .env and modify according to your needs

# Server configuration
SD_HOST_HOST=0.0.0.0
SD_HOST_PORT=8000
SD_HOST_DEBUG=false

# Model configuration
SD_MODEL_NAME=runwayml/stable-diffusion-v1-5
SD_MODEL_PATH=./models/stable-diffusion-v1-5
SD_DEVICE=auto

# Storage paths
SD_OUTPUT_DIR=./output
SD_DATABASE_URL=sqlite+aiosqlite:///./data/sd_host.db

# Security
SD_API_KEY_ENABLED=false
SD_API_KEY=

# Logging
SD_LOG_LEVEL=INFO
SD_LOG_FILE=./logs/sd_host.log

# Performance
SD_PRECISION=fp16
SD_ATTENTION_SLICING=true
SD_MEMORY_EFFICIENT_ATTENTION=true
